{% set sectionno = '0' %}

{% for item in post.get_field('stroken') %}

	{% set sectionno = sectionno + '1' %}
	
	{% if item.acf_fc_layout == 'banner' %}
		{% include 'pagebuilder/banner.twig' %}
	
	{% elseif item.acf_fc_layout == 'blocklinks' %}
		{% include 'pagebuilder/blocklinks.twig' %}
	
	{% elseif item.acf_fc_layout == 'content' %}
		{% include 'pagebuilder/content.twig' %}
	
	{% elseif item.acf_fc_layout == 'projecten' %}
		{% include 'pagebuilder/projecten.twig' %}
		
	{% elseif item.acf_fc_layout == 'quote' %}
		{% include 'pagebuilder/quote.twig' %}
	
	{% elseif item.acf_fc_layout == 'cta' %}
		{% include 'pagebuilder/cta.twig' %}
	
	{% elseif item.acf_fc_layout == 'video' %}
		{% include 'pagebuilder/video.twig' %}
		
	{% endif %}
  
{% endfor %}